    "isCustomPhraseDesc": true,
    "customPhraseDescTS": "// 根据属性名称值编写自定义短语描述，如：\"我的短语是\"+attrs.xxx\n// 使用下拉框组件的变量attrs.xxx有对应的attrs.xxxItems的属性以便获得标签名集合\n// 获取自定义模块的数据名称的函数：getModuleDataName(moduleID:number,dataID:number):string;\n// 获取各种列表内数据名称的函数：getTypeDataListName(typeID: number, dataID: number):string;\n// -- typeID ：0-全局变量 1-全局开关 2-全局字符串 3-玩家变量 4-玩家开关 5-玩家字符串\n//           ：6-场景列表 7-场景对象模型列表 8-图块列表 9-行走图列表 10-行走图动作列表 11-立绘列表 12-立绘表情列表\n//           ：13-公共事件 14-对话框列表 15-动画列表 16-动画信号列表 17-界面列表 18-数据结构列表 19-自定义模块列表\n//           ：20-自定义命令列表 21-自定义行为列表 22-自动元件列表 23-行走图部件列表 24-立绘部件列表\nvar str = `「${attrs.typeItems[attrs.type]}」当`;\n// --- \nif (attrs.isMulKey <= 1) {\n    if (attrs.CTRL) str += \"CTRL+\"\n    if (attrs.SHIFT) str += \"SHIFT+\"\n    if (attrs.ALT) str += \"ALT+\"\n}\nstr += \"「\";\nvar evType = \"\";\nif (attrs.isMulKey == 1) {\n    var keyNames = [];\n    for (var i = 0; i < attrs.keys.length; i++) {\n        keyNames.push(getKeyName(attrs.keys[i]));\n    }\n    str += keyNames.join(\"/\");\n    evType = attrs.evType2Items[attrs.evType2];\n}\nelse if (attrs.isMulKey == 0) {\n    str += getKeyName(attrs.key);\n    evType = attrs.evTypeItems[attrs.evType];\n}\nelse if (attrs.isMulKey == 2) {\n    str += attrs.systemKeyItems[attrs.systemKey];\n    evType = attrs.evTypeItems[attrs.evType];\n}\nelse if (attrs.isMulKey == 3) {\n    var keyNames = [];\n    for (var i = 0; i < attrs.systemKeys.length; i++) {\n        var sysKey = attrs.systemKeys[i];\n        keyNames.push(attrs.systemKeyItems[sysKey]);\n    }\n    str += keyNames.join(\"/\");\n    evType = attrs.evType2Items[attrs.evType2];\n}\n\nstr += \"」\" + evType;\nstr += \"执行：【\" + getFeDataMessage() + \"】\";\nif (attrs.recordListen) {\n    str += ` 监听标识「${attrs.recordListenVar}-${getTypeDataListName(5, attrs.recordListenVar)}」`\n}\nstr;\n\nfunction getKeyName(keyCode: number) {\n    for (var k in Keyboard) {\n        if (Keyboard[k] == keyCode) {\n            return `${k}`;\n        }\n    }\n    return \"--/--\";\n}\nfunction getFeDataMessage() {\n    if (!attrs.eventPage) return \"无\";\n    var startIdx = attrs.eventPage.indexOf(String.fromCharCode(5));\n    var feName = attrs.eventPage.substr(0, startIdx);\n    return feName ? feName : \"未命名片段事件\";\n}",
    "customPhraseDescJS": "var str = \"\\u300C\" + attrs.typeItems[attrs.type] + \"\\u300D\\u5F53\";\nif (attrs.isMulKey <= 1) {\n    if (attrs.CTRL)\n        str += \"CTRL+\";\n    if (attrs.SHIFT)\n        str += \"SHIFT+\";\n    if (attrs.ALT)\n        str += \"ALT+\";\n}\nstr += \"「\";\nvar evType = \"\";\nif (attrs.isMulKey == 1) {\n    var keyNames = [];\n    for (var i = 0; i < attrs.keys.length; i++) {\n        keyNames.push(getKeyName(attrs.keys[i]));\n    }\n    str += keyNames.join(\"/\");\n    evType = attrs.evType2Items[attrs.evType2];\n}\nelse if (attrs.isMulKey == 0) {\n    str += getKeyName(attrs.key);\n    evType = attrs.evTypeItems[attrs.evType];\n}\nelse if (attrs.isMulKey == 2) {\n    str += attrs.systemKeyItems[attrs.systemKey];\n    evType = attrs.evTypeItems[attrs.evType];\n}\nelse if (attrs.isMulKey == 3) {\n    var keyNames = [];\n    for (var i = 0; i < attrs.systemKeys.length; i++) {\n        var sysKey = attrs.systemKeys[i];\n        keyNames.push(attrs.systemKeyItems[sysKey]);\n    }\n    str += keyNames.join(\"/\");\n    evType = attrs.evType2Items[attrs.evType2];\n}\nstr += \"」\" + evType;\nstr += \"执行：【\" + getFeDataMessage() + \"】\";\nif (attrs.recordListen) {\n    str += \" \\u76D1\\u542C\\u6807\\u8BC6\\u300C\" + attrs.recordListenVar + \"-\" + getTypeDataListName(5, attrs.recordListenVar) + \"\\u300D\";\n}\nstr;\nfunction getKeyName(keyCode) {\n    for (var k in Keyboard) {\n        if (Keyboard[k] == keyCode) {\n            return \"\" + k;\n        }\n    }\n    return \"--/--\";\n}\nfunction getFeDataMessage() {\n    if (!attrs.eventPage)\n        return \"无\";\n    var startIdx = attrs.eventPage.indexOf(String.fromCharCode(5));\n    var feName = attrs.eventPage.substr(0, startIdx);\n    return feName ? feName : \"未命名片段事件\";\n}\n",
