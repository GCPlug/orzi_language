    "customShader": "//======================================================================================================\n// SYSTEM START 该段注释由系统自动生成，请勿删改\n//======================================================================================================\n//------------------------------------------------------------------------------------------------------\n// 固定参数\n//------------------------------------------------------------------------------------------------------\n// varying vec2 v_texcoord; //  原始贴图UV坐标\n// uniform sampler2D texture; // 原始贴图\n// uniform float alpha; // 当前对象实际渲染的透明度\n//------------------------------------------------------------------------------------------------------\n// 该材质的自定义参数 判定该变量存在的宏：MU材质编号_变量名称\n//------------------------------------------------------------------------------------------------------\n// uniform float mu6_time; // MU6_time 时间\n// uniform float mu6_trans; // MU6_trans 时间过渡\n// uniform float mu6_sigma; // MU6_sigma 曲率\n// uniform float mu6_trans1; // MU6_trans1 曲率过渡\n// uniform float mu6_strength; // MU6_strength 强度\n// uniform float mu6_trans2; // MU6_trans2 强度过渡\n// uniform float mu6_aspect; // MU6_aspect 纵横比\n//------------------------------------------------------------------------------------------------------\n// shader 代码规则：\n// 1.由于多材质合并shader代码运行，函数名必须以mf${材质ID}_开头以避免函数名称冲突，如 mf6_myFunc\n// 2.入口函数vec4 mf${材质ID}_main 如 vec4 mf6_main(){return gl_FragColor;}\n// 3.原图采样坐标获取：vec2 texCoord = getInRangeTextureCoord(v_texcoord);\n// 4.原图采样：vec4 color = getTextureColor(texCoord);\n// 5.关于自定义贴图的坐标信息：自定义贴图可能是图集中的一部分，提供了相关信息（mu材质编号_变量名_UVInfo）\n//   相对于0~1的坐标系提供了图集的映射信息 vec4 [UV宽度缩放比例, UV高度缩放比例, UV水平偏移量, UV垂直偏移量]\n// 6.变量宏（可用于判定该变量是否满足出现条件）：#ifdef MU材质编号_变量名称、#else、#endif\n// 7.系统宏 IN_GC_EDITOR=是否在编辑器中运行\n// 8.语法：C语言 更多资料请参考GLSL Shader\n//======================================================================================================\n// SYSTEM END\n//======================================================================================================\nvec2 mf6_convertUV(vec2 v_texcoord, float sigma, float aspect, float scale){\n    v_texcoord -= 0.5;\n    v_texcoord /= scale;\n    float r = sqrt(v_texcoord.x * v_texcoord.x * aspect * aspect + v_texcoord.y * v_texcoord.y);\n    float ratio = 1.0 - r * sigma;\n    v_texcoord *= ratio;\n    v_texcoord += 0.5;\n    return v_texcoord;\n}\n\nvec4 mf6_main(){\n\n    #ifdef MU6_trans\n\n    #ifdef MU6_trans1\n    float sigma = mu6_trans1 * mu6_trans;\n    #else\n    float sigma = mu6_sigma * mu6_trans;\n    #endif\n    \n    #else\n\n    #ifdef MU6_trans1\n    float sigma = mu6_trans1 * mu6_time;\n    #else\n    float sigma = mu6_sigma * mu6_time;\n    #endif\n    \n    #endif\n\n    vec2 texCoord = getInRangeTextureCoord(v_texcoord);\n\n    #ifdef MU6_trans2\n    float r = getTextureColor(mf6_convertUV(texCoord, sigma * (1.0 - mu6_trans2), mu6_aspect, 1.0)).r;\n    float b = getTextureColor(mf6_convertUV(texCoord, sigma * (1.0 + mu6_trans2), mu6_aspect, 1.0)).b;\n    #else\n    float r = getTextureColor(mf6_convertUV(texCoord, sigma * (1.0 - mu6_strength), mu6_aspect, 1.0)).r;\n    float b = getTextureColor(mf6_convertUV(texCoord, sigma * (1.0 + mu6_strength), mu6_aspect, 1.0)).b;\n    #endif\n    \n    float g = getTextureColor(mf6_convertUV(texCoord, sigma, mu6_aspect, 1.0)).g;\n    \n    float a = getTextureColor(texCoord).a;\n    vec4 col = vec4(r, g, b, a);\n\n    return col;\n\n\n\n    // #ifdef MU6_trans2\n    // float r = texture2D(texture, mf6_convertUV(v_texcoord, sigma * (1.0 - mu6_trans2), mu6_aspect, 1.0)).r;\n    // float b = texture2D(texture, mf6_convertUV(v_texcoord, sigma * (1.0 + mu6_trans2), mu6_aspect, 1.0)).b;\n    // #else\n    // float r = texture2D(texture, mf6_convertUV(v_texcoord, sigma * (1.0 - mu6_strength), mu6_aspect, 1.0)).r;\n    // float b = texture2D(texture, mf6_convertUV(v_texcoord, sigma * (1.0 + mu6_strength), mu6_aspect, 1.0)).b;\n    // #endif\n    \n    // float g = texture2D(texture, mf6_convertUV(v_texcoord, sigma, mu6_aspect, 1.0)).g;\n    \n    // float a = texture2D(texture, v_texcoord).a;\n    // vec4 col = vec4(r, g, b, a);\n\n    // return col;\n}",
